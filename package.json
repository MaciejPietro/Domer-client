		"@mantine/hooks": "^7.14.2",
		"@tanstack/react-form": "^0.13.6",
		"@tanstack/react-query": "^5.18.0",
		"@tanstack/react-router": "^1.15.7",
		"@tanstack/router-vite-plugin": "^1.15.4",
		"axios": "^1.6.7",
		"clsx": "^2.1.0",
		"dayjs": "^1.11.10",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hot-toast": "^2.4.1",
		"usehooks-ts": "^2.15.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.6.0",
